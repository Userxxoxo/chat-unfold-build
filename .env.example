# DO NOT COMMIT REAL SECRETS TO GIT
# Example environment variables required for Base mainnet deployment and Supabase functions

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Deployer private key (use a secure signer in production)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# Base mainnet RPC endpoint (e.g., https://rpc.base.org)
BASE_RPC_URL=https://rpc.base.org

# BaseScan API key for contract verification
BASESCAN_API_KEY=your_basescan_api_key

# Optional scanner configuration (JSON strings)
# Example DEX_ROUTERS: {"UniswapV2":"0x...","SushiSwap":"0x..."}
DEX_ROUTERS={}
# Example TOKEN_PAIRS: ["WETH/USDC","WETH/DAI"]
TOKEN_PAIRS=[]
# Mapping token symbols to addresses (JSON)
TOKEN_ADDRESSES={}
# Profit threshold percent (sample default 0.5%)
PROFIT_THRESHOLD_PERCENT=0.5
ENABLE_LIVE_EXECUTION=false
DEPLOYED_CONTRACT_ADDRESS=
# Guardrails
MAX_GAS_LIMIT=2000000
MAX_SLIPPAGE_PERCENT=2
EXECUTOR_KILL_SWITCH=false
REMOTE_SIGNER_URL=
SECRET_CONFIRMATION=

# Supabase function runtime may expose other env vars; keep secrets secure and use Supabase's secret manager
